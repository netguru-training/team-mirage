- if projects.empty?
  %p= 'Currently, there are no projects here.'
- else
  %table.table
    %thead
      %tr
        %th= 'Name'
        %th= 'Description'
        %th= 'Finish Time'
        %th= 'Goal'
        %th= 'Current Funds'
        %th= '%'
        %th= 'Status'
    %tbody
      - projects.each do |project|
        %tr.project
          = render project